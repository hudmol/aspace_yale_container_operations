<div class="span12">
  <div class="record-pane yale-container-operations">
    <h1>Bulk Container Operations</h1>

    <div class="well">
      <%= form_tag({}, :method => :get) do %>
        <div class="input-append" style="width:100%">
          <input class="span2 text-filter-field" type="text" name="q" value="<%= params[:q] %>"/>
          <div class="btn-group">
            <button class="btn"><span class=" icon-search"></span></button>
          </div>
        </div>
      <% end %>
    </div>

    <div id="results">
      <% @containers.each do | container | %>
        <div class="result">
          <pre><%= container.inspect %></pre>
        </div>
      <% end %>
    </div>
  </div>
</div>
